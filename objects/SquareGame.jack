  constructor SquareGame new() {
      do SquareGame.drawC(5);
      do SquareGame.drawO(45);
      do SquareGame.drawM(85);
      do SquareGame.drawR(3300);
      do SquareGame.drawA(3338);
      do SquareGame.drawD(3377);
      do SquareGame.drawE(3415);
      do SquareGame.drawmark(3550);
      do SquareGame.drawTank(5700);
      do SquareGame.drawTank(5800);
      do SquareGame.drawTank(5900);
      do SquareGame.drawHuman(6000);
      do Sys.wait(1500);
      do Screen.clearScreen();
      do Output.moveCursor(0,0);
      do Screen.setColor(true);
      do Screen.drawRectangle(16,10,46,40);
      do Output.moveCursor(4,0);
      do Output.printString("You, a KMT spy in CCP");
      do Screen.setColor(true);
      do Screen.drawRectangle(210,10,220,14);
      do Output.moveCursor(4,23);
      do Output.printString("KMT supporters");
      do Output.moveCursor(7,0);
      do Screen.setColor(true);
      do Screen.drawRectangle(16,90,56,130);
      do Output.moveCursor(12,0);
      do Output.printString("President Mao");
      do Output.moveCursor(12,23);
      do Screen.setColor(true);
      do Screen.drawRectangle(210,90,230,110);
      do Output.printString("CCP soldier");
      do SquareGame.draw(5000);
      do SquareGame.drawTank(5200);
      do SquareGame.drawTank(6200);
      do Sys.wait(3000);
      do Screen.clearScreen();
      do Output.moveCursor(0,0);
      do Output.printString("Here are some rules for the game.");
      do Output.println();
      do Output.println();
      do Sys.wait(500);
      do Output.printString("1. You are a spy in CCP, and president Chiang asked you to steal some information from them in order to take down Yan'an");
      do Output.println();
      do Output.println();
      do Sys.wait(500);
      do Output.printString("2.You will get a point if you contact KMT supports successfully.Or you will lose a point if you contact with CCP soldier.");
      do Output.println();
      do Output.println();
      do Sys.wait(500);
      do Output.printString("3.Sometimes you will be recognized as an enemy ,thus you have possibility to avoid being attacked and obtaining score");
      do Output.println();
      do Output.println();
      do Sys.wait(500);
      do Output.printString("4.Sometimes your enemy and support will be split into fractions");
      do Output.println();
      do Output.println();
      do Sys.wait(500);
      do Output.printString("5.Be aware of President Mao ,it is quite dangerous");
      do Output.println();
      do Sys.wait(3000);
      do Screen.clearScreen();
      do Output.moveCursor(0,0);
      do Output.printString("CCP soldier A: Those KMT soldiers are our enemies,and we need to defeat them and let our red flag raised in Beijing!");
      do Output.println();
      do Output.println();
      do Sys.wait(500);
      do Output.printString("CCP soldier B: Bro, watch out! There is a soldier trying to contact the KMT! Damn,We need to kill the betrayer!");
      do Output.println();
      do Output.println();
      do Sys.wait(500);
      do Output.printString("You: We need to hide from the guard and contact our partners");
      do Output.println();
      do Output.println();
      do Sys.wait(3000);
      do Screen.clearScreen();
      let square = Square.new(20, 50, 30);
      let enemy = Enemy.new(100,50,20,20);
      let enemy2 = Enemy.new(200,100,8,8);
      let friend = Enemy.new(290,130,10,4);
      let direction = 0;  // initial state is no movement
      let score = 0;
      return this;
   }
   function int abs(int a,int b){
       var int diff;
       let diff = a-b;
       if(diff < 0){
          let diff = b-a;
       }
       return diff;
   }
   function boolean inrange(int xsquare,int ysquare,int squaresize,int xenemy,int yenemy,int enemy_xlength,int enemy_ylength){
         var boolean in;
         var int maxX1;
         var int maxY1;
         var int maxX2;
         var int maxY2;
         let in = false;
         let maxX1 = (xsquare+squaresize);
         let maxY1 = (ysquare+squaresize);
         let maxX2 = (xenemy+enemy_xlength);
         let maxY2 = (yenemy+enemy_ylength);
         if((maxX1>xenemy)&(maxX2>xsquare)){
            if((maxY1>yenemy)&(maxY2>ysquare)){
                let in = true;
            }
         }
         return in;
     }
   /** Disposes this game. */
   method void dispose() {
      do square.dispose();
      do enemy.dispose();
      do Memory.deAlloc(this);
      return;
   }

   /** Moves the square in the current direction. */
   method void moveSquare() {
      if (direction = 1) { do square.moveUp(); }
      if (direction = 2) { do square.moveDown(); }
      if (direction = 3) { do square.moveLeft(); }
      if (direction = 4) { do square.moveRight(); }
      do Sys.wait(25);  // delays the next movement
      return;
   }
  method void moveEnemy(int direction){
    if (direction = 1) { do enemy.moveUp(); }
    if (direction = 2) { do enemy.moveDown(); }
    if (direction = 3) { do enemy.moveLeft(); }
    if (direction = 4) { do enemy.moveRight(); }
    return;
  }
  method void moveSecondEnemy(int direction){
    if (direction = 1) { do enemy2.moveUp(); }
    if (direction = 2) { do enemy2.moveDown(); }
    if (direction = 3) { do enemy2.moveLeft(); }
    if (direction = 4) { do enemy2.moveRight(); }
    return;
  }
  method void moveFriend(int direction){
    if (direction = 1) { do friend.moveUp(); }
    if (direction = 2) { do friend.moveDown(); }
    if (direction = 3) { do friend.moveLeft(); }
    if (direction = 4) { do friend.moveRight(); }
    return;
  }
   /** Runs the game: handles the user's inputs and moves the square accordingly */
   method void run() {
      var char key;  // the key currently pressed by the user
      var boolean exit;
      var boolean alive1;
      var boolean alive2;
      var boolean alive3;//friend
      var boolean final;
      var boolean win;
      var int enedir;
      var int seed;
      var int seed2;
      var int stepOfNextenemy;
      var int stepfr;
      var int lifeOfking;
      var boolean first;
      let exit = false;
      let alive1 = false;
      let alive2 = false;
      let alive3 = false;
      let final = false;
      let first = false;
      let win = false;
      let lifeOfking = 3;
      do Output.moveCursor(0,0);
      do Output.printString("Score: ");
      do Output.printInt(score);
      do Output.moveCursor(0,10);
      do Output.printString("Life of king: ");
      do Output.printInt(lifeOfking);
      while (~exit) {
         let enedir = Random.randRange(4);
         // waits for a key to be pressed
         while (key = 0) {
            let key = Keyboard.keyPressed();
            do Sys.wait(5);
            do moveSquare();
         }
         if (key = 81)  { let exit = true; }     // q key
         if (key = 131) { let direction = 1; }   // up arrow
         if (key = 133) { let direction = 2; }   // down arrow
         if (key = 130) { let direction = 3; }   // left arrow
         if (key = 132) { let direction = 4; }   // right arrow
         // waits for the key to be released
         while (~(key = 0)) {
            let key = Keyboard.keyPressed();
            let stepOfNextenemy = ((enedir*3)/4);
            let stepfr = ((enedir*3+1)/4);
            do square.draw();
            do enemy.draw();
            do enemy2.draw();
            do friend.draw();
            do Sys.wait(10);
            do moveEnemy(enedir);
            do square.draw();
            do enemy2.draw();
            do friend.draw();
            do Sys.wait(10);
            do moveSecondEnemy(stepOfNextenemy);
            do square.draw();
            do enemy.draw();
            do friend.draw();
            do Sys.wait(10);
            do moveFriend(stepfr);
            do enemy2.draw();
            do square.draw();
            do enemy.draw();
            do moveSquare();
            if(~alive1 & SquareGame.inrange(square.getx(),square.gety(),square.get_size(),enemy.getx(),enemy.gety(),enemy.get_width(),enemy.get_length())){
               do enemy.erase();
               do enemy2.draw();
               do square.draw();
               do friend.draw();
               let score = score-1;
               if(score<0){
                   let exit = true;
               }
               do Output.moveCursor(0,7);
               do Output.printInt(score);
               let alive1 = true;
            }
            if(~alive2 & SquareGame.inrange(square.getx(),square.gety(),square.get_size(),enemy2.getx(),enemy2.gety(),enemy2.get_width(),enemy2.get_length())){
               do enemy2.erase();
               do enemy.draw();
               do square.draw();
               do friend.draw();
               let score = score-1;
               if(score<0){
                  let exit = true;
               }
               do Output.moveCursor(0,7);
               do Output.printInt(score);
               let alive2 = true;
            }
            if(~alive3 & SquareGame.inrange(square.getx(),square.gety(),square.get_size(),friend.getx(),friend.gety(),friend.get_width(),friend.get_length())){
               do friend.erase();
               do enemy2.draw();
               do square.draw();
               do enemy.draw();
               let score = score+1;
               if(score>2){
                 if(~first & ~final){
                   let first = true;
                 }
                 let final = true;
               }
               do Output.moveCursor(0,7);
               do Output.printInt(score);
               let alive3 = true;
            }
         }
         if(alive1){
            let seed = Random.randRange(100);
            let seed2 = Random.randRange(10);
            do enemy.erase();
            do enemy.dispose();
            let enemy = Enemy.new(seed*2+seed2,seed*1+seed2+30,4,4);
            let alive1 = false;
         }
         if(alive2){
            let seed = Random.randRange(100);
            let seed2 = Random.randRange(10);
            do enemy2.erase();
            do enemy2.dispose();
            let enemy2 = Enemy.new(seed*4+seed2,seed*1+seed2+30,8,8);
            let alive2 = false;
         }
         if(alive3){
           let seed = Random.randRange(100);
           let seed2 = Random.randRange(10);
           do friend.erase();
           do friend.dispose();
           let friend = Enemy.new(seed*3+100,seed*1-seed2+90,10,4);
           let alive3 = false;
         }
         if(final){
            let king = King.new(256,125,40);
            if(first){
              do Screen.clearScreen();
              do Output.moveCursor(0,0);
              do Output.printString("supporters A: Wait! What is it?");
              do Output.println();
              do Output.println();
              do Sys.wait(500);
              do Output.printString("CCP soldier A and CCP soldier B: Long live the president Mao!");
              do Output.println();
              do Output.println();
              do Sys.wait(500);
              do Output.printString("You: Damn! That is Mao! What do I need to do now?");
              do Output.println();
              do Output.println();
              do Sys.wait(500);
              do Output.printString("Scout: President Chiang asked you to defeat him just now");
              do Output.println();
              do Output.println();
              do Sys.wait(500);
              do Output.printString("You: All right...");
              do Sys.wait(3000);
              do Screen.clearScreen();
              do square.draw();
              do enemy.draw();
              do enemy2.draw();
              do friend.draw();
              do king.draw();
              do Output.moveCursor(0,0);
              do Output.printString("Score: ");
              do Output.printInt(score);
              do Output.moveCursor(0,10);
              do Output.printString("Life of king: ");
              do Output.printInt(lifeOfking);
              let first = false;
            }

            if(lifeOfking>0){
               if(SquareGame.inrange(square.getx(),square.gety(),square.get_size(),king.getx(),king.gety(),king.get_size(),king.get_size())){
                    let lifeOfking = lifeOfking-1;
                    let score = score-2;
                    do king.draw();
                    do Output.moveCursor(0,7);
                    do Output.printInt(score);
                    do Output.moveCursor(0,10);
                    do Output.printString("Life of king: ");
                    do Output.printInt(lifeOfking);
               }
            }
            if(lifeOfking=0 & score>0){
               let win = true;
               let exit = true;
            }
            if(score < 0){
              let exit = true;
            }
         }
     } // while
     if(win){
       do Screen.clearScreen();
       do Output.moveCursor(0,0);
       do Output.printString("Mao : R! R! R! Inherit my spirits if I die! Take care of my children and my mother!");
       do Output.println();
       do Output.println();
       do Sys.wait(500);
       do Output.printString("CCP soldier A and CCP soldier B: You damn! How dare you to harm our great president!");
       do Output.println();
       do Output.println();
       do Sys.wait(500);
       do Output.printString("Scout: Emergency! President Chiang is hijacked by General Chang and General Yang!");
       do Output.println();
       do Output.println();
       do Sys.wait(500);
       do Output.printString("You: How dare them! Damn Manchurian");
       do Output.println();
       do Output.println();
       do Sys.wait(500);
       do Output.printString("supportersA: Retreat! Back to our HQ to discuss how to save president");
       do Output.println();
       do Output.println();
       do Sys.wait(500);
       do Output.printString("Mao : We will be back again");
       do Sys.wait(3000);
       do Screen.clearScreen();
       do Output.moveCursor(10,15);
       do Output.printString("You win! Thanks for playing");
       do Output.moveCursor(12,17);
       do Output.printString("Score:");
       do Output.printInt(score);
     }
     if(~win){
       do Screen.clearScreen();
       do Output.moveCursor(0,0);
       do Output.printString("You : Damn! I was shot!");
       do Output.println();
       do Sys.wait(500);
       do Output.printString("Square A and Square B: Go to hell!");
       do Output.println();
       do Output.println();
       do Sys.wait(500);
       do Output.printString("(Bang!)");
       do Output.println();
       do Output.println();
       do Output.printString("You: Ah! Ah! Ah!");
       do Output.println();
       do Output.println();
       do Sys.wait(3000);
       do Screen.clearScreen();
       do Output.moveCursor(10,15);
       do Output.printString("You lose! Thanks for playing");
       do Output.moveCursor(12,17);
       do Output.printString("Score:");
       do Output.printInt(score);
     }
     return;
   }
